<h1>Edit Artist</h1>
<p>--------------------------------------------------------</p>
<%= form_for(@artist) do |f| %>
  <p><%= f.label :name %></p>
  <%= f.text_field :name %>
  <p><%= f.label :Add_a_super_nice_img! %></p>
  <%= f.file_field :artistimg %>
  <p><%= f.submit "Save Artist" %></p>
<% end %>
<p>--------------------------------------------------------</p>
  <%= link_to "Cancel", @artist %>
